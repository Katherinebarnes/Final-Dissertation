import serial
import time

# Replace with correct serial port if different
ser = serial.Serial('/dev/serial0', baudrate=9600, timeout=1)

while True:
    line = ser.readline().decode('utf-8', errors='ignore').strip()
    if "$GPGGA" in line or "$GPRMC" in line:
        print("[RAW NMEA]", line)
    time.sleep(1)